{
  "name": "xregexp",
  "version": "4.3.0",
  "description": "Extended regular expressions",
  "homepage": "http://xregexp.com/",
  "author": "Steven Levithan <steves_list@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/slevithan/xregexp.git"
  },
  "keywords": [
    "regex",
    "regexp",
    "regular expression",
    "unicode"
  ],
  "main": "./xregexp-all.js",
  "module": "./index.mjs",
  "files": [
    "index.mjs",
    "tools/output",
    "types/index.d.ts",
    "xregexp-all.js"
  ],
  "scripts": {
    "lint": "eslint . && npm run dtslint",
    "dtslint": "dtslint types",
    "build-unicode-data": "node tools/scripts/block-regex.js && node tools/scripts/category-regex.js && node tools/scripts/property-regex.js && node tools/scripts/script-regex.js",
    "prebuild": "npm run build-unicode-data",
    "build": "rollup -c rollup.config.js",
    "pretest": "npm run build",
    "test": "jasmine JASMINE_CONFIG_PATH=tests/jasmine.json",
    "test-saucelabs": "npm run pretest && npx zuul tests/spec/*.js",
    "test-browser": "npm run test-saucelabs -- --disable-tunnel --local --open",
    "prepack": "npm test"
  },
  "types": "types",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@rollup/plugin-commonjs": "^14.0.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-xregexp": "^1.0.0",
    "dtslint": "^3.6.14",
    "eslint": "^7.6.0",
    "jasmine": "^3.6.1",
    "jsesc": "^3.0.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup": "^2.23.0",
    "typescript": "^3.9.7",
    "unicode-12.1.0": "^0.8.0",
    "unicode-property-value-aliases": "^3.5.0"
  }
}
